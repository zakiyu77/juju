{
  "version": 2,
  "name": "kontok",
  "routes": [
    {
      "src": "/assets/(.*)",
      "dest": "/public/assets/$1"
    },
    {
      "src": "/api/status",
      "dest": "/api/status.js"
    },
    {
      "src": "/api/health",
      "dest": "/api/health.js"
    },
    {
      "src": "/api/download/facebook",
      "dest": "/api/facebook.js"
    },
    {
      "src": "/api/download/tiktok",
      "dest": "/api/tiktok.js"
    },
    {
      "src": "/api/download/youtube",
      "dest": "/api/youtube.js"
    },
    {
      "src": "/api/download/spotify",
      "dest": "/api/spotify.js"
    },
    {
      "src": "/api/download/threads",
      "dest": "/api/threads.js"
    },
    {
      "src": "/api/download/videy",
      "dest": "/api/videy.js"
    },
    {
      "src": "/api",
      "dest": "/api/index.js"
    },
    {
      "src": "/",
      "dest": "/public/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ],
  "functions": {
    "api/*.js": {
      "memory": 1024,
      "maxDuration": 10
    }
  }
}
